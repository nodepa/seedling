{
  "name": "seedling",
  "version": "0.9.0",
  "description": "Modern mobile multi-language literacy - A first-language digital learning tool for adults",
  "homepage": "https://globalseedling.com/get-started",
  "bugs": "https://github.com/nodepa/seedling/issues",
  "repository": "https://github.com/nodepa/seedling",
  "private": true,
  "license": "MIT",
  "keywords": [
    "seedling",
    "literacy",
    "digital-learning",
    "education",
    "edtech",
    "reading",
    "vuejs",
    "typescript"
  ],
  "main": "src/main.js",
  "dependencies": {
    "@capacitor/android": "3.4.1",
    "@capacitor/app": "1.1.0",
    "@capacitor/core": "3.4.1",
    "@capacitor/haptics": "1.1.4",
    "@capacitor/ios": "3.4.1",
    "@capacitor/keyboard": "1.2.2",
    "@capacitor/status-bar": "1.0.8",
    "@ionic/vue": "^6.0.8",
    "@ionic/vue-router": "^6.0.10",
    "register-service-worker": "^1.7.2",
    "roboto-fontface": "*",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12",
    "vuetify": "^3.0.0-alpha.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@capacitor/cli": "3.4.1",
    "@mdi/js": "^6.5.95",
    "@types/jest": "^27.4.0",
    "@vue/cli-plugin-babel": "^5.0.1",
    "@vue/cli-plugin-e2e-cypress": "^5.0.1",
    "@vue/cli-plugin-eslint": "^5.0.1",
    "@vue/cli-plugin-pwa": "^5.0.1",
    "@vue/cli-plugin-router": "^5.0.1",
    "@vue/cli-plugin-typescript": "^5.0.1",
    "@vue/cli-plugin-unit-jest": "^5.0.1",
    "@vue/cli-plugin-vuex": "^5.0.1",
    "@vue/cli-service": "^5.0.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "babel-jest": "^27.5.1",
    "babel-plugin-require-context-hook": "^1.0.0",
    "cypress": "^9.5.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "http-server": "^14.1.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "sass": "^1.49.8",
    "sass-loader": "^12.6.0",
    "ts-jest": "^27.1.3",
    "typescript": "~4.5.5",
    "vue-cli-plugin-vuetify": "~2.4.5"
  },
  "scripts": {
    "build": "vue-cli-service build",
    "buildm": "vue-cli-service build --modern",
    "genass": "cd public && pwd && npx pwa-asset-generator --background 'linear-gradient(#19D2FE, #1E64F0)' --padding '0px' --favicon --single-quotes --type png ../src/assets/logo/logo.svg ./logo",
    "lint": "vue-cli-service lint",
    "prepare": "cd ../ && husky install ./app/.husky",
    "servebuild": "http-server ./dist --gzip --brotli -c-1",
    "start": "vue-cli-service serve",
    "starts": "vue-cli-service serve --https",
    "te": "npm run test:e2e",
    "ted": "npm run test:e2e:dev",
    "teo": "npm run test:e2e:open",
    "tesc": "npm run test:e2e:standalone-cypress",
    "test": "npm run verify",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "test:e2e:dev": "vue-cli-service test:e2e --mode=dev",
    "test:e2e:open": "vue-cli-service test:e2e",
    "test:e2e:standalone-cypress": "CYPRESS_RUN_BINARY=~/Cypress/Cypress npm run test:e2e",
    "test:unit": "vue-cli-service test:unit --watch",
    "test:unit:coverage": "vue-cli-service test:unit --coverage",
    "test:unit:once": "vue-cli-service test:unit",
    "test:unit:verbose": "vue-cli-service test:unit --watch --verbose",
    "tu": "npm run test:unit",
    "tuc": "npm run test:unit:coverage",
    "tuo": "npm run test:unit:once",
    "tuv": "npm run test:unit:verbose",
    "verify": "npm install && npm run lint && npm run test:unit:coverage && npm run test:e2e"
  }
}
