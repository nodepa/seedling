{
  "name": "liaishizi-vue",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mdi/font": "^3.6.95",
    "@mdi/js": "^5.0.45",
    "core-js": "^3.6.4",
    "register-service-worker": "^1.6.2",
    "roboto-fontface": "*",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.5",
    "vuetify": "^2.2.11",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@vue/cli-plugin-babel": "~4.5.4",
    "@vue/cli-plugin-e2e-cypress": "~4.5.4",
    "@vue/cli-plugin-eslint": "~4.5.4",
    "@vue/cli-plugin-pwa": "~4.5.4",
    "@vue/cli-plugin-router": "~4.5.4",
    "@vue/cli-plugin-typescript": "~4.5.4",
    "@vue/cli-plugin-unit-jest": "~4.5.4",
    "@vue/cli-plugin-vuex": "~4.5.4",
    "@vue/cli-service": "~4.5.4",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "1.0.0-beta.31",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "typescript": "~3.9.3",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0",
    "webpack-strip-block": "^0.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "setupFilesAfterEnv": [
      "./tests/unit/jest.setup.js"
    ],
    "testMatch": [
      "**/?(*.)+(test).ts"
    ],
    "moduleNameMapper": {
      "\\.mp3$": "<rootDir>/tests/unit/__mocks__/mp3Mock.js"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "scripts": {
    "build": "vue-cli-service build --modern",
    "test:unit": "vue-cli-service test:unit --watch --verbose",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "lint": "vue-cli-service lint",
    "//servebuild": "'-g/-b'=gzip/brotli compression, '--ssl --cert ../cert/selfssl.crt --key ../cert/selfssl.key'=https",
    "genass": "cd public && pwd && npx pwa-asset-generator --background 'linear-gradient(#19D2FE, #1E64F0)' --padding '0px' --favicon --single-quotes ../src/assets/logo/logo.svg ./logo",
    "servebuild": "vue-cli-service http-server --gzip --brotli ./dist",
    "start": "vue-cli-service serve",
    "starts": "vue-cli-service serve --https",
    "sync": "yarn build && aws s3 sync dist/ s3://vue-liaishizi/ --region=ap-east-1 --delete",
    "te": "yarn test:e2e",
    "ted": "yarn test:e2e:dev",
    "teo": "yarn test:e2e:open",
    "tesc": "yarn test:e2e:standalone-cypress",
    "test:e2e:dev": "vue-cli-service test:e2e --mode=dev",
    "test:e2e:open": "vue-cli-service test:e2e",
    "test:e2e:standalone-cypress": "CYPRESS_RUN_BINARY=~/Cypress/Cypress yarn test:e2e",
    "test:unit:coverage": "vue-cli-service test:unit --coverage",
    "test:unit:once": "vue-cli-service test:unit",
    "test:unit:verbose": "vue-cli-service test:unit --watch --verbose",
    "tu": "yarn test:unit",
    "tuc": "yarn test:unit:coverage",
    "tuo": "yarn test:unit:once",
    "tuv": "yarn test:unit:verbose",
    "verify": "yarn install && yarn lint && yarn test:unit:coverage && yarn test:e2e",
    "vupdate": "vue --version && yarn global upgrade @vue/cli && vue --version && vue upgrade && yarn upgrade"
  }
}
