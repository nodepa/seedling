{
  "name": "zhongzi",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@mdi/font": "^5.9.55",
    "@mdi/js": "^5.0.45",
    "core-js": "^3.6.4",
    "register-service-worker": "^1.6.2",
    "roboto-fontface": "*",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.1.5",
    "vuetify": "^2.5.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "@vue/cli-plugin-babel": "~4.5.10",
    "@vue/cli-plugin-e2e-cypress": "~4.5.13",
    "@vue/cli-plugin-eslint": "~4.5.13",
    "@vue/cli-plugin-pwa": "~4.5.10",
    "@vue/cli-plugin-router": "~4.5.13",
    "@vue/cli-plugin-typescript": "~4.5.13",
    "@vue/cli-plugin-unit-jest": "~4.5.13",
    "@vue/cli-plugin-vuex": "~4.5.10",
    "@vue/cli-service": "~4.5.10",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.2.1",
    "eslint": "^7.29.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^7.4.1",
    "http-server": "^0.12.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.3.2",
    "sass": "^1.19.0",
    "sass-loader": "^10.1.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "typescript": "^4.1.3",
    "vue-cli-plugin-vuetify": "~2.4.1",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.3.0",
    "webpack-strip-block": "^0.3.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "setupFilesAfterEnv": [
      "./tests/unit/jest.setup.js"
    ],
    "testMatch": [
      "**/?(*.)+(test).ts"
    ],
    "moduleNameMapper": {
      "\\.mp3$": "<rootDir>/tests/unit/__mocks__/mp3Mock.js"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "scripts": {
    "build": "vue-cli-service build",
    "buildm": "vue-cli-service build --modern",
    "genass": "cd public && pwd && npx pwa-asset-generator --background 'linear-gradient(#19D2FE, #1E64F0)' --padding '0px' --favicon --single-quotes --type png ../src/assets/logo/logo.svg ./logo",
    "lint": "vue-cli-service lint",
    "servebuild": "http-server ./dist --gzip --brotli -c-1",
    "start": "vue-cli-service serve",
    "starts": "vue-cli-service serve --https",
    "te": "npm run test:e2e",
    "ted": "npm run test:e2e:dev",
    "teo": "npm run test:e2e:open",
    "tesc": "npm run test:e2e:standalone-cypress",
    "test": "npm run verify",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "test:e2e:dev": "vue-cli-service test:e2e --mode=dev",
    "test:e2e:open": "vue-cli-service test:e2e",
    "test:e2e:standalone-cypress": "CYPRESS_RUN_BINARY=~/Cypress/Cypress npm run test:e2e",
    "test:unit": "vue-cli-service test:unit --watch",
    "test:unit:coverage": "vue-cli-service test:unit --coverage",
    "test:unit:once": "vue-cli-service test:unit",
    "test:unit:verbose": "vue-cli-service test:unit --watch --verbose",
    "tu": "npm run test:unit",
    "tuc": "npm run test:unit:coverage",
    "tuo": "npm run test:unit:once",
    "tuv": "npm run test:unit:verbose",
    "verify": "npm install && npm run lint && npm run test:unit:coverage && npm run test:e2e",
    "vupdate": "vue --version && npm update --global @vue/cli && vue --version && vue upgrade && npm update -g"
  }
}
